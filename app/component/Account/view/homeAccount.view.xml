<mvc:View
    controllerName="project2.controller.homeAccount"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m">
	<ScrollContainer height="100%" width="100%" vertical="true" focusable="true">
	<Panel width="auto" class="sapUiResponsiveMargin" accessibleRole="Region">
		<HBox width="100%" height="3rem">
			<Title text="G/L 계정과목"  />
		</HBox>
		<content class="panelbottomline">
			<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="G/L 계정과목 조회"
				subheader="" press="onAccount">
				<TileContent unit="" footer="">
					<ImageContent src="sap-icon://detail-view" />
				</TileContent>
			</GenericTile>
			<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="G/L 계정과목 생성"
				subheader="" press="oncreateAccount">
				<TileContent unit="" footer="">
					<ImageContent src="sap-icon://add-document" />
				</TileContent>
			</GenericTile>
			<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="G/L 계정과목 차트"
				subheader="" press="onchartAccount">
				<TileContent unit="" footer="">
					<ImageContent src="sap-icon://horizontal-bar-chart" />
				</TileContent>
			</GenericTile>
		</content>
	</Panel>
</ScrollContainer>
</mvc:View>