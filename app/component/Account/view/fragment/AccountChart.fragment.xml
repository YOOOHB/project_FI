<core:FragmentDefinition
    xmlns="sap.ui.comp.valuehelpdialog"
    xmlns:m="sap.m"
    xmlns:fb="sap.ui.comp.filterbar"
    xmlns:core="sap.ui.core">
    <ValueHelpDialog
		
        title="계정과목표"
        ok=".onValueHelpOkPress1"
        cancel=".onValueHelpCancelPress1"
        supportRanges="true"
        key="accChart"
        >
        <filterBar>
            <fb:FilterBar advancedMode="true" search=".onFilterBarSearch1" isRunningInValueHelpDialog="true">
                <fb:filterGroupItems>
                    <fb:FilterGroupItem
                        groupName="$INTERNAL$"
                        name="accChart"
                        label="계정과목표"
                        visibleInFilterBar="true">
                        <fb:control>
                            <m:Input name="accChart" />
                        </fb:control>
                    </fb:FilterGroupItem>
                    <fb:FilterGroupItem
                        groupName="$INTERNAL$"
                        name="accContents"
                        label="내역"
                        visibleInFilterBar="true">
                        <fb:control>
                            <m:Input name="accContents" />
                        </fb:control>
                    </fb:FilterGroupItem>
                </fb:filterGroupItems>
            </fb:FilterBar>
        </filterBar>
    </ValueHelpDialog>
</core:FragmentDefinition>
