<core:FragmentDefinition
	xmlns="sap.ui.comp.valuehelpdialog"
	xmlns:m="sap.m"
	xmlns:fb="sap.ui.comp.filterbar"
	xmlns:core="sap.ui.core">
	<ValueHelpDialog
		id= "chartpop"
		title="계정과목표"
		ok=".onValueHelpOkPress"
		cancel=".onCloseDialog1"
		supportRanges="true"
		key="ProductCode"
		descriptionKey="ProductName">
		<filterBar>
			<fb:FilterBar advancedMode="true" search=".onFilterBarSearch" isRunningInValueHelpDialog="true">
				<fb:filterGroupItems>
					<fb:FilterGroupItem
						groupName="__$INTERNAL$"
						name="ProductCode"
						label="Product Code"
						visibleInFilterBar="true">
						<!-- <fb:control>
							<m:Input name="ProductCode" />
						</fb:control> -->
					</fb:FilterGroupItem>
					<fb:FilterGroupItem
						groupName="__$INTERNAL$"
						name="ProductName"
						label="Product Name"
						visibleInFilterBar="true">
						<!-- <fb:control>
							<m:Input name="ProductName" />
						</fb:control> -->
					</fb:FilterGroupItem>
				</fb:filterGroupItems>
			</fb:FilterBar>
		</filterBar>
	</ValueHelpDialog>
</core:FragmentDefinition>
