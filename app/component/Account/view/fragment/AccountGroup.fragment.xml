<core:FragmentDefinition
    xmlns="sap.ui.comp.valuehelpdialog"
    xmlns:m="sap.m"
    xmlns:fb="sap.ui.comp.filterbar"
    xmlns:core="sap.ui.core">
    <ValueHelpDialog
		
        title="계정그룹"
        ok=".onValueHelpOkPress2"
        cancel=".onValueHelpCancelPress2"
        supportRanges="true"
        key="accGroup"
        >
        <filterBar>
            <fb:FilterBar advancedMode="true" search=".onFilterBarSearch2" isRunningInValueHelpDialog="true">
                <fb:filterGroupItems>
                    <fb:FilterGroupItem
                        groupName="$INTERNAL$"
                        name="accGroup"
                        label="계정그룹"
                        visibleInFilterBar="true">
                        <fb:control>
                            <m:Input name="accGroup" />
                        </fb:control>
                    </fb:FilterGroupItem>
                    <fb:FilterGroupItem
                        groupName="$INTERNAL$"
                        name="accChart"
                        label="계정과목표"
                        visibleInFilterBar="true">
                        <fb:control>
                            <m:Input name="accChart" />
                        </fb:control>
                    </fb:FilterGroupItem>
					
                    <fb:FilterGroupItem
                        groupName="$INTERNAL$"
                        name="accMean"
                        label="의미"
                        visibleInFilterBar="true">
                        <fb:control>
                            <m:Input name="accMean" />
                        </fb:control>
                    </fb:FilterGroupItem>
                </fb:filterGroupItems>
            </fb:FilterBar>
        </filterBar>
    </ValueHelpDialog>
</core:FragmentDefinition>
